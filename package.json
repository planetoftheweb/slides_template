{
  "name": "slides_practicalactions",
  "description": "This is a repository for the course Practical Actions by Ray Villalobos for LinkedIn Learning",
  "repository": "http://github.com/planetoftheweb/slides_githubsearch",
  "license": "MIT",
  "version": "2.0.0",
  "scripts": {
    "serve": "vite preview",
    "start": "npm-run-all --parallel dev:*",
    "dev:eleventy": "eleventy --watch",
    "dev:vite": "vite --host 0.0.0.0",
    "build": "run-s prod:*",
    "prod:del": "rimraf docs",
    "prod:serve": "ELEVENTY_ENV=production eleventy --output=./docs/site",
    "prod:copyJS": "copyfiles -u 2 /docs/site/slideList.json ./site/js/slideList.json",
    "prod:vite": "vite build --outDir=../docs --base=./",
    "prod:copy": "copyfiles -u 2 ./src/slides/*.md ./docs/slides",
    "prod:nojekyll": "touch ./docs/.nojekyll"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "animate.css": "^4.1.1",
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.10.5",
    "copyfiles": "^2.4.1",
    "eleventy-plugin-nesting-toc": "^1.3.0",
    "highlight.js": "^11.8.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "marked": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "reveal.js": "^4.5.0",
    "rimraf": "^5.0.1",
    "sass": "^1.66.1",
    "vite": "^4.4.9",
    "webfontloader": "^1.6.28"
  }
}